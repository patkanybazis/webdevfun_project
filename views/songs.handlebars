<div class="principal">

  <h1>ðŸŽµ Music Library</h1>
  <p>Browse through our extensive song collection.</p>

  <div class="songs-list">
    {{#each songs}}
      <div class="song-item">
        <img src="{{this.cover_url}}" alt="album cover for {{this.title}}" />
        <h4>{{this.title}}</h4>
        <p><strong>Duration:</strong> {{this.duration}}</p>
        {{#if this.track_number}}<p><strong>Track:</strong>
            #{{this.track_number}}</p>{{/if}}
      </div>
    {{/each}}
  </div>
</div>